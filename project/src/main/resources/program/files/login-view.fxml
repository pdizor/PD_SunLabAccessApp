<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="program.files.LoginController">
   <children>
      <VBox alignment="CENTER" layoutX="47.0" layoutY="30.0" prefHeight="342.0" prefWidth="522.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="18.0" prefWidth="66.0" text="Username: " />
                  <TextField fx:id="usernameTextField" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="18.0" prefWidth="68.0" text="Password: " />
                  <PasswordField fx:id="passwordTextField" />
               </children>
            </HBox>
            <Button mnemonicParsing="false" onAction="#login" text="Login" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label fx:id="statusLabel" prefHeight="18.0" textAlignment="CENTER" maxWidth="Infinity" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
